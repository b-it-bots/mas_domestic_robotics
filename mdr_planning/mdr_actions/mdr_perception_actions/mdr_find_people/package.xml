<?xml version="1.0"?>
<package>
  <name>mdr_find_people</name>
  <version>0.0.1</version>
  <description>A testing package for finding people with minh's perception stack</description>

  <maintainer email="robotics@inf.h-brs.de">MAS robotics</maintainer>

  <author email="henrik.schnor@smail.inf.h-brs.de">Henrik Schnor</author>

  <license>GPLv3</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>roslint</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>actionlib_msgs</build_depend>
  <build_depend>message_generation</build_depend>
  <build_depend>mas_perception_msgs</build_depend>
  <build_depend>mas_perception_libs</build_depend>

  <run_depend>rospy</run_depend>
  <run_depend>actionlib</run_depend>
  <run_depend>actionlib_msgs</run_depend>
  <run_depend>message_runtime</run_depend>
  <run_depend>mas_perception_msgs</run_depend>
  <run_depend>mas_perception_libs</run_depend>

  <run_depend>numpy</run_depend>
