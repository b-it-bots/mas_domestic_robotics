<?xml version="1.0"?>
<package format="2">
  <name>mdr_find_my_mates</name>
  <version>1.0.0</version>
  <description>State machine and launch files for a RoboCup@Home finy my mates scenario</description>
  <license>GPLv3</license>

  <author email="patrick.nagel@h-brs.de">Patrick Nagel</author>
  <maintainer email="robotics@inf.h-brs.de">MAS robotics</maintainer>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>mas_execution_manager</build_depend>
  <build_depend>mdr_listen_action</build_depend>
  <build_depend>mdr_process_speech_command_action</build_depend>
  <build_depend>mdr_move_base_action</build_depend>
  <build_depend>mdr_navigation_behaviours</build_depend>
  <build_depend>mdr_perception_behaviours</build_depend>

  <run_depend>rospy</run_depend>
  <run_depend>mas_execution_manager</run_depend>
  <run_depend>mdr_listen_action</run_depend>
  <run_depend>mdr_process_speech_command_action</run_depend>
  <run_depend>mdr_move_base_action</run_depend>
  <run_depend>mdr_navigation_behaviours</run_depend>
  <run_depend>mdr_perception_behaviours</run_depend>
